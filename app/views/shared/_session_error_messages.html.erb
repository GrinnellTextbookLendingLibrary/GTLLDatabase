<% if @session != nil %>
<% if @session.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(@session.errors.count, "error") %> prohibited sign in: </h2>
  <p>There were problems with the given fields: </p>
  <ul>
    <% @session.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>
</div>
<% end %>
<% end %>
