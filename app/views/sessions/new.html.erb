<h1>User Sign in</h1>
<div class="infoBlock">
<%= form_for(:session, :url => sessions_path) do |f| %>
 <%= render 'shared/session_error_messages' %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit "Sign in" %>
  </div>
<% end %>
</div>
